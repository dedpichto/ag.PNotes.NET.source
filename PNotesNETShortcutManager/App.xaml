<Application x:Class="PNotesNETShortcutManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="WndMain.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/WPFStandardStyles;component/Styles.xaml"/>
                <ResourceDictionary Source="styles/Window.xaml"/>
                <ResourceDictionary Source="styles/Resources.xaml"/>
                <!--<ResourceDictionary Source="styles/StylesSimple.xaml"/>-->
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
